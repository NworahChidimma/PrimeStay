{
  "expo": {
    "name": "PrimeStay",
    "slug": "prime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/PrimeStay-Icon.png",
    "scheme": "primestay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
     
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/PrimeStay-Logo.png",
        "backgroundImage": "./assets/images/PrimeStay-Logo.png",
        "monochromeImage": "./assets/images/PrimeStay-Logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
     
      "versionCode": 1
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/aerohaven-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/aerohaven-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b501f448-e7c3-432d-b90a-3d6406901e9d"
      }
    },
    "owner": "chidimma3"
      }
}
